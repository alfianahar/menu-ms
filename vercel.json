{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/package.json",
      "use": "@vercel/prisma"
    },
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/web/out/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/src/main.ts"
    }
  ]
}
